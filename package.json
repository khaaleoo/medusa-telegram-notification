{
  "name": "medusa-telegram-notification",
  "version": "0.0.1",
  "description": "A service that sends notifications to Telegram, one or more groups at a time.",
  "author": "Le Xuan Kha (Leo) - khalx.leo@gmail.com",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__ --extensions \".ts,.js\"",
    "watch": "babel -w src --out-dir . --ignore **/__tests__ --extensions \".ts,.js\"",
    "prepare": "cross-env NODE_ENV=production yarn run build"
  },
  "dependencies": {
    "@medusajs/medusa": "^1.6.0",
    "axios": "^1.3.4",
    "medusa-interfaces": "^1.3.3",
    "typeorm": "^0.2.36"
  },
  "repository": "https://github.com/khaaleoo/medusa-telegram-notification",
  "keywords": [
    "telegram",
    "medusa",
    "notification",
    "medusa-telegram-notification",
    "medusa-plugin",
    "medusa-notification-plugin"
  ],
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-typescript": "^7.14.5",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3"
  }
}
